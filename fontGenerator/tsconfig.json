{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
     "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["src"]
}
